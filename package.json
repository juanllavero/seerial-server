{
  "name": "seerial-server",
  "version": "0.7.0",
  "main": "dist/index.js",
  "description": "Media management server for the suite Seerial",
  "author": "Juan Llavero",
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "SeerialMediaServer",
        "executableName": "Seerial Media Server"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "SeerialMediaServer",
            "setupExe": "Seerial Media Server Installer.exe"
          }
        }
      ]
    }
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc && npm run postbundle",
    "dev": "npm run build && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "publish": "npm run build && electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "postbundle": "copyfiles -u 1 assets/* dist/assets/"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.6",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2",
    "ffmetadata": "^1.7.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.3.0",
    "moviedb-promise": "^4.0.7",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^6.1.0",
    "properties-reader": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "36.2.1",
    "@types/cors": "^2.8.17",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^5.0.0",
    "@types/ffprobe-static": "^2.0.3",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.12",
    "@types/properties-reader": "^2.1.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
